generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String   @unique
  password  String
  office    String
  createdAt DateTime @default(now()) @db.Date
  updatedAt DateTime @default(now()) @db.Date
}
